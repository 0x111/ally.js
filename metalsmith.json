{
  "source": "docs",
  "destination": "web",
  "metadata": {},
  "plugins": {
    "metalsmith-markdown-remarkable": {
      "linkify": true
    },

    "metalsmith-packagejson": true,
    "metalsmith/plugins/prepare.js": true,

    "metalsmith-register-helpers": {
      "directory": "metalsmith/helpers/"
    },
    "metalsmith-layouts": {
      "directory": "metalsmith",
      "engine": "handlebars",
      "partials": {
        "table-of-contents": "partials/table-of-contents"
      }
    },

    "metalsmith-broken-link-checker": {
      "warn": true
    }
  }
}